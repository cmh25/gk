d0:.,(`a;1)
d1:.((`a;1);(`b;2))
d2:.((`a;1);(`b;2);(`c;3))
d3:.,(`a;10)
d4:.((`a;10);(`b;20))
d5:.((`a;10);(`b;20);(`c;30))
d0 (|,)\ d3
d0 (|,)\ d3,d4
d0 (|,)\ d3,d4,d5
(d0;d1) (|,)\ d3
(d0;d1) (|,)\ d3,d4
(d0;d1) (|,)\ d3,d4,d5
(d0;d1;d2) (|,)\ d3
(d0;d1;d2) (|,)\ d3,d4
(d0;d1;d2) (|,)\ d3,d4,d5

nul (|,)\ 1
nul (|,)\ 1 2
nul (|,)\ 1 2 3
(;) (|,)\ 1
(;) (|,)\ 1 2
(;) (|,)\ 1 2 3
(;;) (|,)\ 1
(;;) (|,)\ 1 2
(;;) (|,)\ 1 2 3

