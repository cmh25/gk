f:{x}
p:{x+y}
j:{x,y}

f'!10
p'[!10;!10]
2 3 p\ 4 5
2 3 p/ 4 5
2 3 p\/ 4 5 6
2 3 p/\ 4 5 6
p'!10
p/!10
p\!10

f'0.0+!10
p'[0.0+!10;0.0+!10]
2 3.0 p\ 4 5.0
2 3.0 p/ 4 5.0
2 3.0 p\/ 4 5 6.0
2 3.0 p/\ 4 5 6.0
p'0.0+!10
p/0.0+!10
p\0.0+!10

f'"asdf"
j'["asdf";"wxyz"]
"asdf" j\ "wxyz"
"asdf" j/ "wxyz"
"as" j\/ "wxyz"
"as" j/\ "wxyz"
j'"asdfwxyz"
j/"asdfwxyz"
j\"asdfwxyz"

f'`a`s`d`f
j'[`a`s`d`f;`w`x`y`z]
`a`s`d`f j\ `w`x`y`z
`a`s`d`f j/ `w`x`y`z
`a`s j\/ `w`x`y`z
`a`s j/\ `w`x`y`z
j'`a`s`d`f`w`x`y`z
j/`a`s`d`f`w`x`y`z
j\`a`s`d`f`w`x`y`z

f'(1;2.0;"a";`b)
j'[(1;2.0;"a";`b);(3;4.0;"c";`d)]
(1;2.0;"a";`b) j\ (3;4.0;"c";`d)
(1;2.0;"a";`b) j/ (3;4.0;"c";`d)
(1;2.0) j\/ (3;4.0;"c";`d)
(1;2.0) j/\ (3;4.0;"c";`d)
j'(1;2.0;"a";`b;3;4.0;"c";`d)
j/(1;2.0;"a";`b;3;4.0;"c";`d)
j\(1;2.0;"a";`b;3;4.0;"c";`d)

