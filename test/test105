5 in !10
5 11 lin !10
1 2 3 4 5 dvl 2 4
1 2 3 dv 2

5.0 in 0.0+!10
5 11.0 lin 0.0+!10
1 2 3 4 5.0 dvl 2 4.0
1 2 3.0 dv 2.0

"a" in "asdf"
"ab" lin "asdf"
"asdf" dvl "sd"
"asdf" dv "s"

`a in `a`s`d`f
`a`b lin `a`s`d`f
`a`s`d`f dvl `s`d
`a`s`d`f dv `s

(.,`a,1) in (.,`a,1;.,`b,2)
(.,`a,1;.,`b,2) lin (.,`a,1;.,`b,2;.,`c,3)
(.,`a,1;.,`b,2;.,`c,3;.,`d,4) dvl (.,`b,2;.,`c,3)
(.,`a,1;.,`b,2;.,`c,3;.,`d,4) dv .,`b,2

nul in (;;;)
(;;) lin (;;;)
(;;) dvl (;;;)
nul dv (;;;)

{} in ({};{x})
({};{x};{z}) lin ({};{x};{y})
({};{x};{y};{z}) dvl ({x};{y})
({};{x};{y};{z}) dv {x}

in[5;!10]
lin[5 11;!10]
dvl[1 2 3 4 5;2 4]
dv[1 2 3;2]

in[5.0;0.0+!10]
lin[5 11.0;0.0+!10]
dvl[1 2 3 4 5.0;2 4.0]
dv[1 2 3.0;2.0]

in["a";"asdf"]
lin["ab";"asdf"]
dvl["asdf";"sd"]
dv["asdf";"s"]

in[`a;`a`s`d`f]
lin[`a`b;`a`s`d`f]
dvl[`a`s`d`f;`s`d]
dv[`a`s`d`f;`s]

in[(.,`a,1);(.,`a,1;.,`b,2)]
lin[(.,`a,1;.,`b,2);(.,`a,1;.,`b,2;.,`c,3)]
dvl[(.,`a,1;.,`b,2;.,`c,3;.,`d,4);(.,`b,2;.,`c,3)]
dv[(.,`a,1;.,`b,2;.,`c,3;.,`d,4);.,`b,2]

in[nul;(;;;)]
lin[(;;);(;;;)]
dvl[(;;);(;;;)]
dv[nul;(;;;)]

in[{};({};{x})]
lin[({};{x};{z});({};{x};{y})]
dvl[({};{x};{y};{z});({x};{y})]
dv[({};{x};{y};{z});{x}]

